{
  "name": "gulp-devstack",
  "version": "1.0.0",
  "description": "Gulp DevStack for develop.",
  "main": "gulpfile.babel.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fixmerge": "npm run installforce && npm run generatepackagelock && echo NPM merge fixed.",
    "installforce": "npm i -f --no-package-lock",
    "generatepackagelock": " npm i --package-lock-only",
    "develop": "gulp",
    "production": "gulp --env production",
    "bower": "node_modules/.bin/bower install",
    "bower-installer": "node node_modules/bower-installer/bower-installer",
    "postinstall": "npm run bower",
    "styleguide-devel": "stylemark -i app -o guide -c stylemark.yml -b -w",
    "styleguide": "stylemark -i app -o guide -c stylemark.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/partyk/gulp-devstack.git"
  },
